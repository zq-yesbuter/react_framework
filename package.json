{
  "name": "recruit",
  "version": "1.0.0",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env ESLINT=none API=DEV NODE_ENV=development webpack-dev-server --progress --color --compress --config=config/webpack.development.js --mode development",
    "start:mock": "cross-env API=MOCK NODE_ENV=development webpack-dev-server --progress --color --compress --config=config/webpack.development.js --mode development",
    "start:dev": "cross-env API=DEV NODE_ENV=development webpack-dev-server --progress --color --compress --config=config/webpack.development.js --mode development",
    "start:pre": "cross-env API=PRE NODE_ENV=development webpack-dev-server --progress --color --compress --config=config/webpack.development.js --mode development",
    "build": "sh ./build.sh",
    "build:sh": "cross-env ESLINT=none webpack --config=config/webpack.production.js --hide-modules true --mode production",
    "build:dll": "webpack  --config=config/webpack.dll.js --hide-modules true --mode production",
    "build:dev": "cross-env NODE_ENV=production BUILD_ENV=development webpack --config=config/webpack.production.js --hide-modules true --mode production",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true ESLINT=none roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src mock && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src mock && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "@babel/runtime-corejs2": "^7.5.5",
    "@types/history": "^4.7.6",
    "@types/prop-types": "^15.7.3",
    "antd": "^3.26.16",
    "awesome-typescript-loader": "^5.2.1",
    "bizcharts": "^3.1.3-beta.1",
    "bizcharts-plugin-slider": "^2.0.1",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.4",
    "dva": "^2.4.1",
    "dva-loading": "^3.0.20",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.14",
    "enquire-js": "^0.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "marked": "^0.6.2",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-drawer-menu": "^0.5.0",
    "react": "^16.13.1",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-fittext": "^1.0.0",
    "ts-loader": "^7.0.5",
    "url-polyfill": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^6.2.0",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.35",
    "@types/react-document-title": "^2.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dva-hmr": "^0.4.2",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^5.0.0",
    "gh-pages": "^1.0.0",
    "happypack": "^5.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "import-sort-parser-typescript": "^6.0.0",
    "lint-staged": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mockjs": "^1.0.1-beta3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "1.11.1",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.8.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --wirter",
      "git add"
    ],
    "**/*.{js,jsx}": "lint-staged:js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
